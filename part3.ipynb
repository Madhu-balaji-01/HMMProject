    "    for i in range(N):\n",
    "        stop.append(max(stopmax_copy))\n",
    "        state_ans.append(states[stopmax.index(stop[i]) // N])\n",
    "        stopmax_copy[stopmax.index(stop[i])] = -999999999.999\n",
    "    scores[n][state_u] = tuple((state_ans[m], stop[m]) for m in range(N))\n",
    "    \n",
    "      \n",
    "    # Backtracking path\n",
